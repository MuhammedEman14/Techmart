{
  "name": "techmart-backend",
  "version": "1.0.0",
  "description": "TechMart E-commerce Analytics Dashboard - Backend API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "node src/migrations/runMigrations.js",
    "seed": "node src/seeders/runSeeders.js",
    "clean": "node src/migrations/cleanDatabase.js",
    "reset": "npm run clean && npm run migrate && npm run seed",
    "generate-data": "node src/generateCSV.js",
    "verify": "node src/verify.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "ecommerce",
    "analytics",
    "dashboard",
    "rest-api",
    "fraud-detection"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.0.1",
    "mysql2": "^3.6.5",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "sequelize": "^6.35.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}
